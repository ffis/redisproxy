{
	"redis": {
		"host": "localhost",
		"port": 6379,
		"no_ready_check": true
	},
	"server": {
		"port": 8080,
		"bind": "127.0.0.1",
		"https": false,
		"options": {
			"key": "privatekey.pem",
			"cert": "cert.pem",
			"passphrase": ""
		}
	},
	"jwtdecoder": {
        "secret": "12345678901234567890",
        "format": "base64",
        "notify": "http://localhost:1304/api/0/users/:username/log"
    },
	"debug": false,
	"restproxyplugins": [
		"compression", "jwt", "api"
	]
}
	